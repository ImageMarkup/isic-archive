if segmentation.id
  - var creator = segmentation.get('creator')
  table.table.table-condensed.isic-segmentation-display-table
    tbody
      tr
        td Created
        td= created
      tr
        td Skill
        td= segmentation.get('skill')
      tr
        td Creator
        td= formatUser(creator)
